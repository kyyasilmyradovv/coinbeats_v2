{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/**/*"]
      }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "server.js" }],
  "installCommand": "pnpm install --no-frozen-lockfile",
  "buildCommand": "prisma generate"
}
